{
  "name": "@carped99/react-ol",
  "private": false,
  "type": "module",
  "version": "0.0.5-alpha.3",
  "description": "React Components for OpenLayers - Personal Use",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    },
    "./ext": {
      "types": "./dist/ol-ext.d.ts",
      "import": "./dist/ol-ext.es.js",
      "require": "./dist/ol-ext.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "pnpm dev",
    "test": "vitest",
    "lint": "eslint",
    "coverage": "vitest run --coverage",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "pnpx rimraf --glob **/node_modules **/dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "ol": ">=10.0.0",
    "ol-ext": "^4.0.24",
    "react": ">=18",
    "react-dom": ">=18",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "3.2.3",
    "@eslint/js": "^9.17.0",
    "@microsoft/api-extractor": "^7.48.1",
    "@microsoft/tsdoc": "^0.15.1",
    "@storybook/addon-essentials": "8.4.7",
    "@storybook/addon-interactions": "8.4.7",
    "@storybook/addon-onboarding": "8.4.7",
    "@storybook/blocks": "8.4.7",
    "@storybook/builder-vite": "^8.4.7",
    "@storybook/react": "8.4.7",
    "@storybook/react-vite": "8.4.7",
    "@storybook/test": "8.4.7",
    "@testing-library/react": "^16.1.0",
    "@types/node": "^22.10.5",
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitest/coverage-v8": "2.1.8",
    "@vitest/ui": "2.1.8",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-storybook": "^0.11.2",
    "eslint-plugin-tsdoc": "^0.4.0",
    "globals": "^15.14.0",
    "jotai": "^2.11.0",
    "jsdom": "^25.0.1",
    "ol": "^10.3.1",
    "ol-ext": "^4.0.24",
    "prettier": "^3.4.2",
    "recoil": "^0.7.7",
    "rimraf": "^6.0.1",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.13.1",
    "storybook": "8.4.7",
    "ts-node": "^10.9.2",
    "typedoc": "^0.27.6",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8",
    "zustand": "^5.0.2"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "dependencies": {
    "fast-deep-equal": "^3.1.3",
    "type-fest": "^4.31.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
