{
  "name": "@carped99/ol-react-ext",
  "private": false,
  "type": "module",
  "version": "0.0.1-alpha.11",
  "description": "React Components for OpenLayers - Personal Use",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.es.js",
  "main": "./dist/index.cjs.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b && vite build",
    "dev": "pnpm dev",
    "test": "vitest",
    "lint": "eslint",
    "coverage": "vitest run --coverage",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "pnpx rimraf --glob **/node_modules **/dist"
  },
  "peerDependencies": {
    "@carped99/ol-react": "0.0.1-alpha.23",
    "@carped99/ol-util": "0.0.1-alpha.35",
    "ol": ">=10.0.0",
    "ol-ext": "^4.0.24",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/ol-ext": "npm:@siedlerchr/types-ol-ext@^3.6.1",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "ol": "catalog:",
    "ol-ext": "catalog:"
  },
  "packageManager": "pnpm@9.15.1",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "dependencies": {
    "@carped99/ol-react": "0.0.1-alpha.22",
    "fast-deep-equal": "^3.1.3",
    "type-fest": "^4.33.0"
  }
}
